<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.liang.user.dao.LoginMapper">

	<select id="select_user" parameterType="com.liang.user.bean.T_MALL_USER" resultType="com.liang.user.bean.T_MALL_USER">
		select * from t_mall_user where yh_mch = #{yh_mch} and yh_mm = #{yh_mm}
	</select>
	
</mapper>
